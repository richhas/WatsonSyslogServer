{
	"version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}\\SyslogServer\\SyslogServer.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "reveal": "silent"
            },
            "problemMatcher": "$msCompile"
        }

        {
            "label": "print workspaceFolder",
            "type": "shell",
            "command": "echo",
            "args": ["${workspaceFolder}"],
            "problemMatcher": []
        }
    ]
}